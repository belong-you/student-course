/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
icon {
  display: inline-block;
  background-size: 100%;
}
.fl{
    float: left;
}
.fr{
    float: right;
}
.clearFix::after{
    content: '';
    display: block;
    clear: both;
}